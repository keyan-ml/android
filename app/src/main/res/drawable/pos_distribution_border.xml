<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 圆角 -->
    <corners
        android:radius="1dp"/><!-- 设置圆角半径 -->

    <!-- 间隔 -->
    <padding
        android:left="1dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="1dp"/><!-- 各方向的间隔 -->

    <solid
        android:color="#FF9192"
        />

    <!-- 描边 -->
    <stroke
        android:width="1dp"
        android:color="#555"/><!-- 描边 -->

</shape>